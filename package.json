{
  "name": "getiprange",
  "version": "1.0.0",
  "description": "Simple utility to convert CIDR notation to an array of IP addresses",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_PATH=. nyc --reporter=html --reporter=text mocha --recursive",
    "jshint": "jshint *.js test/*.js",
    "code-style": "jscs -p airbnb --fix *.js test/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "check-coverage": "nyc check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "open-coverage": "open coverage/index.html"
  },
  "keywords": [
    "CIDR",
    "IP"
  ],
  "author": "@JoeScho <joeschofield@live.co.uk>",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "jscs": "^3.0.7",
    "jshint": "^2.9.5",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0"
  },
  "pre-commit": [
    "jshint",
    "code-style",
    "test",
    "check-coverage"
  ],
  "jshintConfig": {
    "boss": true,
    "node": true,
    "strict": false,
    "smarttabs": true,
    "maxlen": 100,
    "newcap": false,
    "undef": true,
    "unused": true,
    "onecase": true,
    "indent": 2,
    "sub": true,
    "esversion": 8,
    "predef": [
      "it",
      "describe",
      "before"
    ]
  },
  "dependencies": {
    "ip": "^1.1.5"
  }
}
